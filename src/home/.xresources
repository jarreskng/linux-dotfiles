! -*- mode: conf-xdefaults -*-

!-----------------------------------------------------------------------
!                         xft settings
!-----------------------------------------------------------------------

Xft.dpi:		96
Xft.autohint:		0
Xft.antialias:		true
Xft.rgba:		rgb
Xft.hinting:		true
Xft.lcdfilter:		lcddefault
Xft.hintstyle:		hintsfull

!-----------------------------------------------------------------------
!                         urxvt settings
!-----------------------------------------------------------------------



URxvt.font:		xft:JetBrains Mono Medium:size=10,xft:Symbols Nerd Font:style=1000-em
URxvt.boldFont:  	xft:JetBrains Mono Medium:size=10
URxvt.italicFont:  	xft:JetBrains Mono Medium:size=10
URxvt.boldItalicfont:  	xft:JetBrains Mono Medium:size=10

URxvt.letterSpace: 	-1
URxvt.lineSpace: 	0
URxvt.depth: 		24
URxvt.iso14755: 	false
URxvt.internalBorder:  	12
URxvt.scrollBar: 	false

