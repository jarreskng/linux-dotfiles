! -*- mode: conf-xdefaults -*-

!-----------------------------------------------------------------------
!                         xft settings
!-----------------------------------------------------------------------

Xft.dpi:		96
Xft.autohint:		0
Xft.antialias:		true
Xft.rgba:		rgb
Xft.hinting:		true
Xft.lcdfilter:		lcddefault
Xft.hintstyle:		hintsfull

